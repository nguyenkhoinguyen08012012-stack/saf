<Module>
<ModulePrefs title="Google.com" />
<Content type="html"><![CDATA[


<!DOCTYPE html>
<html lang="en-us">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>classroom.google.com</title>
    <style>
      html, body {
        margin: 0;
        padding: 0;
        height: 100%;
        background: #000;
        overflow: hidden;
      }
      #unity-container {
        width: 100%;
        height: 100%;
        position: fixed;
        top: 0;
        left: 0;
        display: flex;
        justify-content: center;
        align-items: center;
      }
      #unity-canvas {
        width: 100%;
        height: 100%;
        background: #000;
        display: block;
      }
      #unity-loading-bar,
      #unity-footer,
      #unity-logo,
      #unity-webgl-logo,
      #unity-build-title,
      #unity-fullscreen-button {
        display: none !important;
      }
      #unity-mobile-warning {
        display: none;
      }
    </style>
  </head>
  <body>
    <div id="unity-container">
      <canvas id="unity-canvas"></canvas>
    </div>

    <script>
      var buildUrl = "https://cdn.jsdelivr.net/gh/ertemr33/saf@main/Build";
      var loaderUrl = buildUrl + "/oopsNoBrakes.loader.js";

      // --- ðŸ”¹ ParÃ§alarÄ± birleÅŸtirme fonksiyonu ---
      async function mergeParts(baseFileName, partCount) {
        const parts = [];
        for (let i = 1; i <= partCount; i++) {
          const url = `${buildUrl}/${baseFileName}.part${i}`;
          const response = await fetch(url);
          if (!response.ok) throw new Error("Dosya yÃ¼klenemedi: " + url);
          const blob = await response.blob();
          parts.push(blob);
        }
        return new Blob(parts);
      }

      // --- ðŸ”¹ .data.gz ve .wasm.gz dosyalarÄ±nÄ± birleÅŸtir ---
      async function prepareUnityFiles() {
        const dataBlob = await mergeParts("oopsNoBrakes.data.gz", 3); // 4 parÃ§aydÄ±
        const wasmBlob = await mergeParts("oopsNoBrakes.wasm.gz", 3); // 3 parÃ§aydÄ±

        const dataUrl = URL.createObjectURL(dataBlob);
        const wasmUrl = URL.createObjectURL(wasmBlob);
        return { dataUrl, wasmUrl };
      }

      // --- ðŸ”¹ Unity yÃ¼kleme iÅŸlemi ---
      prepareUnityFiles().then(({ dataUrl, wasmUrl }) => {
        var config = {
          dataUrl: dataUrl,
          frameworkUrl: buildUrl + "/oopsNoBrakes.framework.js",
          codeUrl: wasmUrl,
          streamingAssetsUrl: "StreamingAssets",
          companyName: "TheJaspel",
          productName: "Oops! No Brakes",
          productVersion: "1",
        };

        var canvas = document.querySelector("#unity-canvas");

        var script = document.createElement("script");
        script.src = loaderUrl;
        script.onload = () => {
          createUnityInstance(canvas, config, (progress) => {
            // progress bar yok, gerek yok
          }).then((unityInstance) => {
            // Otomatik tam ekran yap
            unityInstance.SetFullscreen(1);
          }).catch((message) => {
            alert(message);
          });
        };
        document.body.appendChild(script);
      }).catch((err) => {
        alert("Dosya yÃ¼klenemedi: " + err.message);
      });
    </script>

    <!-- Sticky Bottom Center Ad (728x90) with Smooth Slide-Out and Reappearance -->
<style>

</script>

  </body>
</html>




